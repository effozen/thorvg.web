{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/lottie-player.ts"],
  "out": "./docs",
  "tsconfig": "./tsconfig.json",
  "plugin": [],
  "theme": "default",
  "name": "@thorvg/thorvg.web",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": false,
  "excludeInternal": true,
  "excludeNotDocumented": true,
  "readme": "./README.md",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Components",
    "Types",
    "Enums",
    "Utilities",
    "*",
    "Other"
  ],
  "groupOrder": [
    "Variables",
    "Functions",
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Enums",
    "*"
  ],
  "sort": ["source-order"],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "cleanOutputDir": true,
  "hideGenerator": false,
  "disableSources": false,
  "gitRevision": "main",
  "gitRemote": "origin",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}